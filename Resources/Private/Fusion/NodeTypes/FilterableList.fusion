    cards = Neos.Neos:ContentCollection {
        nodePath = 'cards'
    }

    categories = Neos.Neos:ContentCollection {
        nodePath = 'categories'
    }

    groups = Neos.Neos:ContentCollection {
        nodePath = 'groups'
    }
}
